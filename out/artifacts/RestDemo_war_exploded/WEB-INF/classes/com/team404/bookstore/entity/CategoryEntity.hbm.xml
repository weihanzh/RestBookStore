<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.team404.bookstore.entity.CategoryEntity" table="Category" schema="bookstore">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="category">
            <column name="category" sql-type="varchar(200)" length="200"/>
        </property>
    </class>

    <query name="ListCategoryQuery">FROM CategoryEntity </query>
</hibernate-mapping>